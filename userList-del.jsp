<script  id="tbl" type="text/html">
    <h1>用户列表展示</h1>
    <div class="table-responsive">
        <table class="table table-bordered">
            <thead>
            <tr>
                <th>编号</th>
                <th>姓名</th>
                <th>年龄</th>
                <th>邮箱</th>
            </tr>
            </thead>
            <tbody>
            {{each obj.list }}
            <tr>
                <th scope="row">{{$value.id}}</th>
                <td>{{$value.name}}</td>
                <td>{{$value.age}}</td>
                <td>{{$value.email}}</td>
            </tr>
            {{/each}}
            </tbody>
        </table>
    </div>
    <nav>
        <ul class="pagination">
            {{if obj.pageNum==1 }}
             <li class="disabled">
                 <a href="#!">
                    <span><i class="mdi mdi-chevron-left"></i></span>
                 </a>
             </li>
             {{else}}
             <li >
                <a onclick="findByPage({{obj.pageNum-1}})">
                    <span><i class="mdi mdi-chevron-left"></i></span>
                </a>
            </li>
            {{/if}}
            {{each obj.navigatepageNums}}
               {{if obj.pageNum== $value}}
                 <li class="active"><a onclick="findByPage({{$value}})">{{$value}}</a></li>
                {{else}}
                  <li ><a onclick="findByPage({{$value}})">{{$value}}</a></li>
               {{/if}}
            {{/each}}

            {{if obj.pageNum==obj.pages }}
             <li class="disabled">
                 <a href="#!">
                    <span><i class="mdi mdi-chevron-right"></i></span>
                 </a>
             </li>
             {{else}}
             <li >
                <a onclick="findByPage({{obj.pageNum+1}})">
                    <span><i class="mdi mdi-chevron-right"></i></span>
                </a>
            </li>
            {{/if}}
        </ul>
    </nav>
</script>
 <script>
     function findByPage(pageNum){
         $.ajax({
             url:"${pageContext.request.contextPath}/user/queryAll",
             type:"get",
             data:{pageNum:pageNum},
             dataType:"json",
             success:function (resp){
                console.log(resp)
                 var temphtml=template("tbl",{obj:resp.data});
                $("#tableList").html(temphtml);
             }
         })
     }
     $(function (){
         findByPage(1);
     })
 </script>
